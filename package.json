{
  "name": "kalisto-solana-explorer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "cd server && npm run start",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "build": "tsc || true && vite build || true",
    "build:server": "cd server && npm install --legacy-peer-deps && (tsc || true)",
    "start": "cd server && [[ -n \"$VITE_QUICKNODE_RPC_URL\" && -z \"$QUICKNODE_RPC_URL\" ]] && export QUICKNODE_RPC_URL=$VITE_QUICKNODE_RPC_URL; NODE_ENV=production node dist/index.js",
    "postinstall": "(npm run build || true) && (npm run build:server || true)",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "engines": {
    "node": ">=18.x"
  },
